  "info": {
    "title": "Customer Favorites Products",
    "description": "This is a custom favourite product",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "accessToken": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "in": "header"
      }
    },
    "schemas": {
      "ResponseClientsDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": "1",
            "description": "Cliente ID"
          },
          "name": {
            "type": "string",
            "example": "NOME CLIENTE",
            "description": "Cliente ID"
          },
          "email": {
            "type": "string",
            "example": "cliente@email.com",
            "description": "Cliente ID"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-06-21T03:35:39.548Z",
            "description": "Data de Criação do registro"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-06-21T03:35:39.548Z",
            "description": "Data de Modificação do registro"
          }
        },
        "required": [
          "name",
          "email"
        ]
      },
      "CreateInputClientDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Nome Cliente",
            "description": "Nome do cliente"
          },
          "email": {
            "type": "string",
            "example": "cliente@email.com",
            "description": "Email utilizado para gera token"
          },
          "password": {
            "type": "string",
            "minLength": 6,
            "example": "SuaSenh@123",
            "description": "Senha de acesso para gera token"
          }
        },
        "required": [
          "name",
          "email",
          "password"
        ]
      },
      "UpdateInputClientDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Nome Cliente",
            "description": "Nome do cliente"
          },
          "email": {
            "type": "string",
            "example": "cliente@email.com",
            "description": "Email utilizado para gera token"
          },
          "password": {
            "type": "string",
            "minLength": 6,
            "example": "SuaSenh@123",
            "description": "Senha de acesso para gera token"
          }
        }
      },
      "Rating": {
        "type": "object",
        "properties": {
          "rate": {
            "type": "number",
            "example": "1"
          },
          "count": {
            "type": "number",
            "example": "1"
          }
        },
        "required": [
          "rate",
          "count"
        ]
      },
      "ResponseFavoriteItensDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": "1"
          },
          "title": {
            "type": "string",
            "example": "Nome do produto"
          },
          "image": {
            "type": "string",
            "example": "imagem.jpg"
          },
          "price": {
            "type": "number",
            "example": "1.50"
          },
          "rating": {
            "$ref": "#/components/schemas/Rating"
          }
        },
        "required": [
          "id",
          "title",
          "image",
          "price",
          "rating"
        ]
      },
      "AddProductFavoriteClienteDto": {
        "type": "object",
        "properties": {
          "clientId": {
            "type": "number",
            "example": 1,
            "description": "Nome do cliente"
          },
          "productId": {
            "type": "number",
            "example": 1,
            "description": "ID do produto"
          }
        },
        "required": [
          "clientId",
          "productId"
        ]
      },
      "ResponseFavoriteDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": "1"
          },
          "customerId": {
            "type": "number",
            "example": "1"
          },
          "productId": {
            "type": "number",
            "example": "1"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-06-22T18:00:01.720Z"
          }
        },
        "required": [
          "id",
          "customerId",
          "productId",
          "createdAt"
        ]
      },
      "AuthSignInDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "password"
        ]
      }
    }
  }
