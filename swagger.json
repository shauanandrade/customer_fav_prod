                "example": {
                  "access_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6InNoYXVhbkBnbWFpbC5jb20iLCJpYXQiOjE3NTA1NjAzNzYsImV4cCI6MTc1MDU2Mzk3Nn0"
